{
    "title": "Wingbot.ai",
    "description": "Enterprise bot tooling",
    "author": "Wingbot.ai",
    "variables": {
        "twitter": "https://twitter.com/wingbotai"
    },
    "plugins": [ "theme-default", "collapsible-menu", "anchors", "highlight"],
